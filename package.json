{
    "name": "jeremiak.com",
    "version": "1.0.0",
    "description": "Personal little corner of the internet",
    "main": "index.js",
    "scripts": {
        "build": "npm-run-all --parallel build:11ty build:sass",
        "build:11ty": "npx @11ty/eleventy",
        "build:sass": "npx sass _sass/styles.scss:_site/css/styles.css",
        "dev": "npm-run-all --parallel watch:11ty watch:sass",
        "watch:11ty": "npx @11ty/eleventy --serve",
        "watch:sass": "npx sass _sass/styles.scss:_site/css/styles.css --watch",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jeremiak/jeremiak.com.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jeremiak/jeremiak.com/issues"
    },
    "homepage": "https://github.com/jeremiak/jeremiak.com#readme",
    "devDependencies": {
        "@11ty/eleventy": "^2.0.0-beta.1",
        "cheerio": "^1.0.0-rc.12",
        "npm-run-all": "^4.1.5",
        "sass": "^1.45.0",
        "striptags": "^3.2.0"
    }
}